    "express": "^4.21.2",
    "faiss-node": "^0.5.1",
    "multer": "^1.4.5-lts.1",
    "onnxruntime-node": "^1.20.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/multer": "^1.4.12",
    "@types/nodemon": "^1.19.6",
    "@types/xlsx": "^0.0.36",
    "nodemon": "^3.1.9"
  }
}
